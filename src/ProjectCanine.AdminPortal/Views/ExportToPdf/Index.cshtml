@model IEnumerable<ProjectCanine.AdminPortal.ViewModels.ExportGridRow>
@{
    ViewBag.Title = "Test Results to Export";
}
<h2>@ViewBag.Title</h2>
<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.HandlerName)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.DogName)
		</th>
		@*<th>
				@Html.DisplayNameFor(model => model.PassedTest)
			</th>*@
		<th>
			@Html.DisplayNameFor(model => model.TestDate)
		</th>
		<th></th>
	</tr>
	@foreach (var item in Model) {
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.HandlerName)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.DogName)
			</td>
			@*<td>
					@Html.DisplayFor(modelItem => item.PassedTest)
				</td>*@
			<td>
				@Html.DisplayFor(modelItem => item.TestDate)
			</td>

			<td>
				@Html.ActionLink("Export", "Details", new { id=item.TestResultId }) @*|*@
				@*@Html.ActionLink("Delete", "Delete", new { id=item.Id })*@
			</td>
		</tr>
}
</table>
