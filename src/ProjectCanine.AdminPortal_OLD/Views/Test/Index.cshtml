@model IEnumerable<ProjectCanine.AdminPortal_OLD.ViewModels.TestGridRow>
@{
	ViewBag.Title = "View Tests";
}
<h2>@ViewBag.Title</h2>
<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.ShortName)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Name)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.LastEditedBy)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.LastEditedDate)
		</th>
		<th></th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.ShortName)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Name)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.LastEditedBy)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.LastEditedDate)
			</td>
			<td>
				@Html.ActionLink("Details", "Details", new { id = item.Id })
			</td>
		</tr>
	}
</table>
